<h1>New article</h1>

<%= form_for(@article) do |f| %>
  <p><%= f.label :title %></p>
  <p><%= f.text_field :title %></p>
  <p><%= f.label :content %></p>
  <p><%= f.text_area :content, size: "70x5" %></p>
  <p><%= f.submit %></p>
<% end %>

<p><%= link_to "Back to list of Articles", articles_path  %></p>
